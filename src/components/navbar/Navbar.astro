---
// const {title , text} = Astro.props
import "./Navbar.css";
---
<div class="navbar_content">
  <div class="navbar_items">
    <div class="logo_and_title">
      <svg
        class="Logo_icon"
        version="1.1"
        id="Icons"
        xmlns="http://www.w3.org/2000/svg"
        xmlns:xlink="http://www.w3.org/1999/xlink"
        x="0px"
        y="0px"
        viewBox="0 0 32 32"
        style="enable-background:new 0 0 32 32;"
        xml:space="preserve"
      >
        <rect
          x="10"
          y="26"
          class="st0"
          width="4"
          height="3"
          stroke="black"
          stroke-width="0.25"
          fill="white"></rect>
        <rect
          x="7"
          y="4"
          transform="matrix(6.123234e-17 -1 1 6.123234e-17 3 14)"
          class="st0"
          width="3"
          height="3"
          stroke="black"
          stroke-width="0.25"
          fill="white"></rect>
        <rect
          x="22"
          y="4"
          transform="matrix(6.123234e-17 -1 1 6.123234e-17 18 29)"
          class="st0"
          width="3"
          height="3"
          stroke="black"
          stroke-width="0.25"
          fill="white"></rect>
        <rect
          x="18"
          y="26"
          class="st0"
          width="4"
          height="3"
          stroke="black"
          stroke-width="0.25"
          fill="white"></rect>
        <polygon
          class="st0"
          points="28,17 28,14 25,14 25,11 22,11 22,7 19,7 19,11 13,11 13,7 10,7 10,11 7,11 7,14 4,14 4,17 1,17 1,20 1,26 4,26 4,20 7,20 7,26 10,26 10,23 11,23 21,23 22,23 22,26 25,26 25,20 28,20 28,26 31,26 31,20 31,17"
          stroke="black"
          stroke-width="0.25"
          fill="white"></polygon>
        <rect
          x="10"
          y="16"
          class="st0"
          width="3"
          height="3"
          stroke="black"
          stroke-width="0.25"
          fill="white"></rect>
        <rect
          x="19"
          y="16"
          class="st0"
          width="3"
          height="3"
          stroke="black"
          stroke-width="0.25"
          fill="white"></rect>
      </svg>
      <span class='Logo_tittle' style="background-image: linear-gradient(to right, #0ea5e9, #22d3ee); -webkit-background-clip: text; color: transparent; font-size: 2rem;">JuanD' Portafolio</span>
    </div>
    <nav class="links_container">
      <a href="https://wa.link/2dbi3j" class="Links">Contacto</a>
      <a href="https://github.com/Escobarq" class="Links">Github</a>
      <a href="https://www.facebook.com/juandavid.escobarquezada" class="Links">Facebook</a>
    </nav>
  </div>
</div>

